#!/bin/bash
cd ~/swift/go
if [ $? -ne 0 ]; then
  echo "You don't have the hummingbird branch checked out"
  exit 1
fi
set -e
mkdir -p $GOPATH/src/github.com/openstack
rm $GOPATH/src/github.com/openstack/swift 2> /dev/null || true
ln -s /vagrant/swift $GOPATH/src/github.com/openstack/swift
cd $GOPATH/src/github.com/openstack/swift/go
make get test all
